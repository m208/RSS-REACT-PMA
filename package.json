    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-easy-edit": "^1.15.0",
    "react-hook-form": "^7.39.3",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.5"
  }
}
